{
    "debug": true,
    "log-level": "debug",
    "experimental": true,
    "data-root": "/var/lib/docker",
    "storage-driver": "overlay2",

    "log-driver": "json-file",
    "log-opts": {
      "max-size": "100m",
      "max-file": "3"
